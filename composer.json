{
    "name": "mnarushevich/auth-service",
    "type": "project",
    "description": "Auth Service",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.5",
        "aws/aws-sdk-php": "^3.359.11",
        "bugsnag/bugsnag-laravel": "^2.0",
        "darkaonline/l5-swagger": "^8.6",
        "filament/filament": "^3.3.2",
        "laravel/framework": "^v12.38.1",
        "laravel/octane": "^2.13.1",
        "laravel/sanctum": "^4.2.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.5",
        "mateusjunges/laravel-kafka": "3.x-dev",
        "mnarushevich/laravel-audit-logs": "^0.0.2",
        "spatie/laravel-permission": "^6.23",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "larastan/larastan": "^3.8",
        "laravel/boost": "^1.8",
        "laravel/pail": "^1.2",
        "laravel/pint": "dev-main",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-faker": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "rector/rector": "^2.2",
        "soloterm/solo": "^0.5.0",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "rector": "rector",
        "lint": "pint",
        "test:rector": "rector --dry-run",
        "test:lint": "pint --test",
        "test:lint:fix": "pint",
        "test:types": "phpstan analyse --memory-limit=512M",
        "test:unit": "pest --colors=always --stop-on-failure",
        "test:type-coverage": "php -d memory_limit=2G ./vendor/bin/pest --type-coverage --compact --min=95",
        "test": [
            "@test:type-coverage",
            "@test:rector",
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}